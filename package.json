{
  "name": "zbay",
  "productName": "Zbay",
  "build": {
    "productName": "Zbay",
    "appId": "Zbay",
    "protocols": {
      "name": "electron-deep-linking",
      "schemes": [
        "zbay"
      ]
    },
    "mac": {
      "category": "public.app-category.social-networking",
      "files": [
        "dist/**/*",
        "build/icon.*",
        "package.json"
      ],
      "extraResources": "zcash/**/*",
      "publish": {
        "provider": "s3",
        "bucket": "zbay-release"
      }
    },
    "win": {
      "target": "NSIS",
      "files": [
        "dist/**/*",
        "build/icon.*",
        "package.json"
      ],
      "extraResources": [
        "zcash/**/*",
        {
          "from": "../../../.zcash-params",
          "to": "zcash/ZcashParams"
        }
      ]
    },
    "linux": {
      "target": [
        "deb"
      ],
      "files": [
        "dist/**/*",
        "build/icon.*",
        "package.json"
      ],
      "publish": {
        "provider": "s3",
        "bucket": "zbay-release-linux"
      }
    },
    "extraResources": "zcash/**/*"
  },
  "homepage": "https://gitlab.rumblefishdev.com/holmes/zbay",
  "version": "0.6.3-dev-2",
  "description": "Distributed goods and bulletin board",
  "main": "dist/main.js",
  "scripts": {
    "dist": "aws s3 sync s3://zbay-zcash-binaries/darwin zcash/darwin  &&   npm run build:prod && electron-builder -p always --mac",
    "distlinux": "aws s3 sync s3://zbay-zcash-binaries/linux zcash/linux && npm run build:prod && electron-builder -p always --linux",
    "snaplinux": "aws s3 sync s3://zbay-zcash-binaries/linux zcash/linux && npm run build:prod && electron-builder --linux snap",
    "distwin": " aws s3 sync s3://zbay-zcash-binaries/win32 zcash/win32 && npm run pullResources && npm run build:prod && electron-builder --win",
    "pullResources": "./zcash/win32/zcash-fetch-params",
    "lint": "standard",
    "test": "npm run lint && jest",
    "test:watch": "npm run lint && jest --watchAll",
    "build:prod": "concurrently \"npm run build:main:prod\" \"npm run build:renderer:prod\"",
    "build:main": "npm run lint && webpack --config webpack/webpack.config.main.dev.js",
    "build:main:prod": "npm run lint && webpack --config webpack/webpack.config.main.prod.js",
    "build:renderer:prod": "npm run lint && webpack --config webpack/webpack.config.renderer.prod.js",
    "prestart": "npm run build:main",
    "start": "npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electron .",
    "start:renderer": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.config.renderer.dev.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": {
    "name": "Bartlomiej Lisiecki/Norbert Bodziony",
    "email": "rumblefish@rumblefish.pl"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-backgrounds": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "axios-mock-adapter": "^1.16.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "electron": "^4.0.3",
    "electron-builder": "^21.2.0",
    "electron-debug": "^2.1.0",
    "react-virtualized": "^9.21.1",
    "electron-devtools-installer": "^2.2.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "keccak": "^2.0.0",
    "node-sass": "^4.12.0",
    "on-build-webpack": "^0.1.0",
    "react-hot-loader": "^4.6.5",
    "react-test-renderer": "^16.7.0",
    "sass-loader": "^8.0.0",
    "secp256k1": "^3.7.1",
    "standard": "^12.0.1",
    "storybook-addon-material-ui": "^0.9.0-alpha.19",
    "storybook-react-router": "^1.0.5",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ],
    "globals": [
      "each"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/shared/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/shared/testing/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testRegex": "src/.*\\.test\\.js$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@material-ui/core": "^4.4.0",
    "@material-ui/icons": "^4.2.1",
    "axios": "^0.18.0",
    "bech32": "^1.1.3",
    "bignumber.js": "^8.1.1",
    "binance-api-node": "^0.9.5",
    "buttercup": "^2.10.0",
    "classnames": "^2.2.6",
    "country-region-data": "^1.4.7",
    "electron-updater": "^4.0.6",
    "formik": "^1.5.4",
    "formik-material-ui": "0.0.22",
    "fs-extra": "^8.1.0",
    "identity-obj-proxy": "^3.0.0",
    "immutable": "^4.0.0-rc.12",
    "jest-each": "^24.0.0",
    "luxon": "^1.11.4",
    "notistack": "^0.8.2",
    "prop-types": "^15.7.1",
    "qrcode.react": "^0.9.3",
    "ramda": "^0.26.1",
    "react": "^16.9.0",
    "react-alice-carousel": "^1.15.3",
    "react-copy-to-clipboard": "^5.0.1",
    "react-countdown-now": "^2.1.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.7.0",
    "react-jdenticon": "0.0.8",
    "react-measure": "^2.2.4",
    "react-redux": "^6.0.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-debounced": "^0.5.0",
    "redux-immutable": "^4.0.0",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "typeface-roboto": "0.0.54",
    "yup": "^0.27.0"
  }
}
