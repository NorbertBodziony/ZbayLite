{
  "name": "zbay",
  "productName": "zBay",
  "build": {
    "productName": "zbay"
  },
  "version": "0.1.0",
  "description": "Distributed goods and bulletin board",
  "main": "dist/main.js",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && jest",
    "test:watch": "npm run lint && jest --watchAll",
    "build:main": "npm run lint && webpack --config webpack/webpack.config.main.dev.js",
    "prestart": "npm run build:main",
    "start": "npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electron .",
    "start:renderer": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.config.renderer.dev.js"
  },
  "author": "Bartlomiej Lisiecki",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "electron": "^4.0.3",
    "electron-debug": "^2.1.0",
    "electron-devtools-installer": "^2.2.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.0.0",
    "on-build-webpack": "^0.1.0",
    "react-hot-loader": "^4.6.5",
    "react-test-renderer": "^16.7.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ],
    "globals": [
      "each"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/shared/setupTests.js"
    ],
    "testRegex": "src/.*\\.test\\.js$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "bignumber.js": "^8.1.1",
    "buttercup": "^2.10.0",
    "classnames": "^2.2.6",
    "immutable": "^4.0.0-rc.12",
    "jest-each": "^24.0.0",
    "luxon": "^1.11.4",
    "prop-types": "^15.7.1",
    "ramda": "^0.26.1",
    "react": "^16.7.0",
    "react-countdown-now": "^2.1.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.7.0",
    "react-measure": "^2.2.4",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-immutable": "^4.0.0",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "typeface-roboto": "0.0.54"
  }
}
