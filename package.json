{
  "name": "zbay",
  "version": "0.1.0",
  "description": "Distributed goods and bulletin board",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && jest",
    "test:watch": "npm run lint && jest --watchAll",
    "build:main": "npm run lint && webpack --config webpack/webpack.config.main.dev.js",
    "prestart": "npm run build:main",
    "start": "npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electron dist/main.js",
    "start:renderer": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.config.renderer.dev.js"
  },
  "author": "Bartlomiej Lisiecki",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "electron": "^4.0.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.0.0",
    "on-build-webpack": "^0.1.0",
    "react-hot-loader": "^4.6.5",
    "react-test-renderer": "^16.7.0",
    "standard": "^12.0.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testRegex": "src/.*\\.test\\.js$"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
